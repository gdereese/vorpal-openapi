    "jasmine-ts": "^0.2.1",
    "prettier": "^1.10.2",
    "tslint": "5.8.0",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cli-spinner": "^0.2.7",
    "lodash": "^4.17.5",
    "swagger-client": "^3.4.11",
    "vorpal": "^1.12.0"
